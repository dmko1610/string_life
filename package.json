    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "expo-atlas": "^0.4.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "metro": "^0.82.0",
    "prettier": "^3.3.3",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
